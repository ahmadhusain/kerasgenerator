citHeader("To cite kerasgenerator in publications use:")

citEntry(
  entry = "Misc",
  title = "Data Generator for R interface to Keras",
  author = personList(
    person(given = "R. Dimas Bagas", family = "Herlambang")
  ),
  year = "2019",
  url = "http://github.com/bagasbgy/kerasgenerator/",
  textVersion =
    paste(
      "Garrett Grolemund, Hadley Wickham (2011).",
      "Data Generator for R interface to Keras.",
      "Journal of Statistical Software, 40(3), 1-25.",
      "URL http://www.jstatsoft.org/v40/i03/."
    )
)
